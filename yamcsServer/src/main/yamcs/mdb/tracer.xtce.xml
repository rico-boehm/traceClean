            <Parameter parameterTypeRef="temp_type" name="TEMP_sensor_7"/>
            <Parameter parameterTypeRef="temp_type" name="TEMP_sensor_8"/>
            <Parameter parameterTypeRef="temp_type" name="TEMP_sensor_9"/>
            <Parameter parameterTypeRef="temp_type" name="TEMP_sensor_10"/>
            <Parameter parameterTypeRef="temp_type" name="TEMP_sensor_11"/>
            <Parameter parameterTypeRef="temp_type" name="TEMP_sensor_12"/>
            <Parameter parameterTypeRef="temp_type" name="TEMP_sensor_13"/>
            <Parameter parameterTypeRef="temp_type" name="TEMP_sensor_14"/>
            <Parameter parameterTypeRef="temp_type" name="TEMP_sensor_15"/>
            <Parameter parameterTypeRef="temp_type" name="TEMP_sensor_16"/>
            <Parameter parameterTypeRef="temp_type" name="TEMP_sensor_17"/>
            <Parameter parameterTypeRef="temp_type" name="TEMP_sensor_18"/>
            <Parameter parameterTypeRef="temp_type" name="TEMP_sensor_19"/>
