    <!-- COMMAND SECTION-->
    <CommandMetaData>
        <ArgumentTypeSet>
            <IntegerArgumentType name="uint_8" signed="false">
                <UnitSet/>
                <IntegerDataEncoding sizeInBits="8"/>
            </IntegerArgumentType>
            
            <IntegerArgumentType name="uint_16" signed="false">
                <UnitSet/>
                <IntegerDataEncoding sizeInBits="16"/>
            </IntegerArgumentType>
            
