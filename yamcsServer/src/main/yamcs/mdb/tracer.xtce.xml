            <Parameter parameterTypeRef="current_8" name="CURR_5"/>
            <Parameter parameterTypeRef="current_8" name="CURR_12"/>
            <Parameter parameterTypeRef="flow_24" name="FLOW_gas"/>
            <Parameter parameterTypeRef="flow_24" name="FLOW_liq"/>
            <Parameter parameterTypeRef="pressure_24" name="PRESS_tank"/>
            <Parameter parameterTypeRef="pressure_24" name="PRESS_surf"/>

            <!-- HULL SENSOR DATASET-->
            <Parameter parameterTypeRef="switch_LOW" name="LOW_state"/>
            <Parameter parameterTypeRef="temp_type" name="TEMP_sensor_1"/>
            <Parameter parameterTypeRef="temp_type" name="TEMP_sensor_2"/>
            <Parameter parameterTypeRef="temp_type" name="TEMP_sensor_3"/>
