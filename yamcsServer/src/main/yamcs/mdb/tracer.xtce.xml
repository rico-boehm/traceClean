                <BaseContainer containerRef="Packet_header">
					<RestrictionCriteria>
						<ComparisonList>
							<Comparison value="0x42" parameterRef="SYNC_1" />
                            <Comparison value="0xac" parameterRef="SYNC_2" />
							<Comparison value="0xf2" parameterRef="ID"/>
						</ComparisonList>
					</RestrictionCriteria>
				</BaseContainer>
            </SequenceContainer>
            <!-- END OF HULL SENSOR PACKET 1-->

            <!-- HULL SENSOR PACKET 2-->
            <SequenceContainer name="Hull_sensor_packet_2" shortDescription="Telemetry data from the second set of hull sensors">
                <EntryList>
                    <ParameterRefEntry parameterRef="LO_state"/>
                    <ParameterRefEntry parameterRef="SODS_state"/>
                    <ParameterRefEntry parameterRef="SOE_state"/>
                    <ParameterRefEntry parameterRef="LOW_state"/>
                    <ParameterRefEntry parameterRef="FILL"/>
                    <ParameterRefEntry parameterRef="EXP_timer"/>
                    <ParameterRefEntry parameterRef="TEMP_sensor_10"/>
                    <ParameterRefEntry parameterRef="TEMP_sensor_11"/>
                    <ParameterRefEntry parameterRef="TEMP_sensor_12"/>
                    <ParameterRefEntry parameterRef="TEMP_sensor_13"/>
                    <ParameterRefEntry parameterRef="TEMP_sensor_14"/>
