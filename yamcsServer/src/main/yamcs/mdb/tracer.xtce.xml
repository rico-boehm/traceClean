    <TelemetryMetaData>
    <!-- THIS CONTAINS ALL THE DATA FOR TELEMETRY (DOWNLINK)-->    
        <ParameterTypeSet>
        <!-- THIS CONTAINS DEFINITIONS OF PARAMETER TYPES-->
            <AggregateParameterType name="CCSDS_Packet_ID_Type">
                <MemberList>
                    <Member name="Version" typeRef="CCSDS_Version_Type" />
                    <Member name="Type" typeRef="CCSDS_Type_Type" />
                    <Member name="SecHdrFlag" typeRef="CCSDS_Sec_Hdr_Flag_Type" />
                    <Member name="APID" typeRef="CCSDS_APID_Type" />
                </MemberList>
            </AggregateParameterType>
            
            <IntegerParameterType name="CCSDS_Version_Type" signed="false">
                <UnitSet />
                <IntegerDataEncoding sizeInBits="3" />
            </IntegerParameterType>
            
            <BooleanParameterType name="CCSDS_Type_Type" zeroStringValue="TM" oneStringValue="TC">
                <UnitSet />
                <IntegerDataEncoding sizeInBits="1" />
            </BooleanParameterType>
            
            <BooleanParameterType name="CCSDS_Sec_Hdr_Flag_Type" zeroStringValue="NotPresent" oneStringValue="Present">
                <UnitSet />
                <IntegerDataEncoding sizeInBits="1" />
            </BooleanParameterType>
            
            <IntegerParameterType name="CCSDS_APID_Type" signed="false">
                <UnitSet />
                <IntegerDataEncoding sizeInBits="11" />
            </IntegerParameterType>
            
            <AggregateParameterType name="CCSDS_Packet_Sequence_Type">
                <MemberList>
                    <Member name="GroupFlags" typeRef="CCSDS_Group_Flags_Type" />
                    <Member name="Count" typeRef="CCSDS_Source_Sequence_Count_Type" />
                </MemberList>
            </AggregateParameterType>
            
            <EnumeratedParameterType name="CCSDS_Group_Flags_Type">
                <UnitSet />
                <IntegerDataEncoding sizeInBits="2" />
                <EnumerationList>
                    <Enumeration value="0" label="Continuation" />
                    <Enumeration value="1" label="First" />
                    <Enumeration value="2" label="Last" />
                    <Enumeration value="3" label="Standalone" />
                </EnumerationList>
            </EnumeratedParameterType>
            
            <IntegerParameterType name="CCSDS_Source_Sequence_Count_Type" signed="false">
                <UnitSet />
                <IntegerDataEncoding sizeInBits="14" />
            </IntegerParameterType>
            
            <IntegerParameterType name="CCSDS_Packet_Length_Type" signed="false" initialValue="0">
                <UnitSet>
                    <Unit description="Size">Octets</Unit>
                </UnitSet>
                <IntegerDataEncoding sizeInBits="16" />
            </IntegerParameterType>
            
            <IntegerParameterType name="uint_4">
                <UnitSet/>
                <IntegerDataEncoding sizeInBits="4"/>
            </IntegerParameterType>
            
            <IntegerParameterType name="uint_8">
                <UnitSet/>
                <IntegerDataEncoding sizeInBits="8"/>
            </IntegerParameterType>
            
            <IntegerParameterType name="uint_16">
                <UnitSet/>
                <IntegerDataEncoding sizeInBits="16"/>
            </IntegerParameterType>
            
            <IntegerParameterType name="uint_32">
                <UnitSet/>
                <IntegerDataEncoding sizeInBits="32"/>
            </IntegerParameterType>
            
            <BinaryParameterType name="fill">
                <BinaryDataEncoding>
                    <SizeInBits>
                        <FixedValue>3</FixedValue>
                    </SizeInBits>
                </BinaryDataEncoding>
            </BinaryParameterType>

            <EnumeratedParameterType name="switch_1_on">
                <UnitSet/>
                <IntegerDataEncoding sizeInBits="1"/>
                <EnumerationList>
                    <Enumeration value="1" label="ON" />
                    <Enumeration value="0" label="OFF" />
                </EnumerationList>
            </EnumeratedParameterType>
            
            <EnumeratedParameterType name="switch_UPP">
                <UnitSet/>
                <IntegerDataEncoding sizeInBits="2"/>
                <EnumerationList>
                    <Enumeration value="1" label="Testing" />
                    <Enumeration value="2" label="Experiment Ready" />
                    <Enumeration value="3" label="Experiment Active" />
                </EnumerationList>
            </EnumeratedParameterType>
            
            <EnumeratedParameterType name="switch_LOW">
                <UnitSet/>
                <IntegerDataEncoding sizeInBits="2"/>
                <EnumerationList>
                    <Enumeration value="1" label="Testing" />
                    <Enumeration value="2" label="Operating" />
                </EnumerationList>
            </EnumeratedParameterType>
            
            <EnumeratedParameterType name="switch_4_open">
                <UnitSet/>
                <IntegerDataEncoding sizeInBits="4"/>
                <EnumerationList>
                    <Enumeration value="1" label="CLOSED" />
                    <Enumeration value="0" label="OPEN" />
                </EnumerationList>
            </EnumeratedParameterType>
            
            <EnumeratedParameterType name="switch_4_yes">
                <UnitSet/>
                <IntegerDataEncoding sizeInBits="4"/>
                <EnumerationList>
                    <Enumeration value="1" label="YES" />
                    <Enumeration value="0" label="NO" />
                </EnumerationList>
            </EnumeratedParameterType>
            
            <EnumeratedParameterType name="switch_4_on">
                <UnitSet/>
                <IntegerDataEncoding sizeInBits="4"/>
                <EnumerationList>
                    <Enumeration value="1" label="ON" />
                    <Enumeration value="0" label="OFF" />
                </EnumerationList>
            </EnumeratedParameterType>
            
            <EnumeratedParameterType name="switch_8">
                <UnitSet/>
                <IntegerDataEncoding sizeInBits="8"/>
                <EnumerationList>
                    <Enumeration value="1" label="ON" />
                    <Enumeration value="0" label="OFF" />
                </EnumerationList>
            </EnumeratedParameterType>
            
            <EnumeratedParameterType name="power_mux_type">
                <UnitSet/>
                <IntegerDataEncoding sizeInBits="8"/>
                <EnumerationList>
                    <Enumeration value="1" label="RXIF" />
                    <Enumeration value="0" label="BATTERY" />
                </EnumerationList>
            </EnumeratedParameterType>

            <IntegerParameterType name="flow_24">
                <UnitSet>
                    <Unit>g/s</Unit>
                </UnitSet>
                <IntegerDataEncoding sizeInBits="24"/>
            </IntegerParameterType>
            
            <IntegerParameterType name="current_16">
                <UnitSet>
                    <Unit>mA</Unit>
                </UnitSet>
                <IntegerDataEncoding sizeInBits="16"/>
            </IntegerParameterType>

            <IntegerParameterType name="current_8">
                <UnitSet>
                    <Unit>mA</Unit>
                </UnitSet>
                <IntegerDataEncoding sizeInBits="8"/>
            </IntegerParameterType>
            
            <IntegerParameterType name="voltage_16">
                <UnitSet>
                    <Unit>mV</Unit>
                </UnitSet>
                <IntegerDataEncoding sizeInBits="16"/>
            </IntegerParameterType>
            
            <IntegerParameterType name="celsius_8">
                <UnitSet>
                    <Unit>°C</Unit>
                </UnitSet>
                <IntegerDataEncoding sizeInBits="8">
                    <DefaultCalibrator>
                        <PolynomialCalibrator>
                            <Term coefficient="256" exponent="1" />
                        </PolynomialCalibrator>
                    </DefaultCalibrator>
                </IntegerDataEncoding>
            </IntegerParameterType>
            
            <FloatParameterType name="timer_type">
                <UnitSet>
                    <Unit>s</Unit>
                </UnitSet>
                <IntegerDataEncoding encoding="unsigned" sizeInBits="32">
                    <DefaultCalibrator>
                        <PolynomialCalibrator>
                            <Term coefficient="0.001" exponent="1" />
                        </PolynomialCalibrator>
                    </DefaultCalibrator>
                </IntegerDataEncoding>
            </FloatParameterType>
            
            <FloatParameterType name="temp_type">
                <UnitSet>
                    <Unit>mV</Unit>
                </UnitSet>
                <IntegerDataEncoding encoding="twosComplement" sizeInBits="24">
                    <DefaultCalibrator>
                        <PolynomialCalibrator>
                            <Term coefficient="0.00000447035" exponent="1" />
                        </PolynomialCalibrator>
                    </DefaultCalibrator>
                </IntegerDataEncoding>
            </FloatParameterType>
            
            <IntegerParameterType name="celsius_16">
                <UnitSet>
                    <Unit>°C</Unit>
                </UnitSet>
                <IntegerDataEncoding sizeInBits="16"/>
            </IntegerParameterType>
            
            <IntegerParameterType name="accel_16">
                <UnitSet>
                    <Unit>m/s²</Unit>
                </UnitSet>
                <IntegerDataEncoding sizeInBits="16"/>
            </IntegerParameterType>
            
            <IntegerParameterType name="accel_8">
                <UnitSet>
                    <Unit>m/s²</Unit>
                </UnitSet>
                <IntegerDataEncoding sizeInBits="8"/>
            </IntegerParameterType>
            
            <IntegerParameterType name="roll_8">
                <UnitSet>
                    <Unit>Hz</Unit>
                </UnitSet>
                <IntegerDataEncoding sizeInBits="8"/>
            </IntegerParameterType>
            
            <IntegerParameterType name="pressure_32">
                <UnitSet>
                    <Unit>mbar</Unit>
                </UnitSet>
                <IntegerDataEncoding sizeInBits="32"/>
            </IntegerParameterType>
            
            <IntegerParameterType name="pressure_24">
                <UnitSet>
                    <Unit>mbar</Unit>
                </UnitSet>
                <IntegerDataEncoding encoding="twosComplement" sizeInBits="24">
                    <DefaultCalibrator>
                        <PolynomialCalibrator>
                            <Term coefficient="9.4045" exponent="1" />
                        </PolynomialCalibrator>
                    </DefaultCalibrator>
                </IntegerDataEncoding>
            </IntegerParameterType>
            
            <IntegerParameterType name="humidity_32">
                <UnitSet>
                    <Unit>%</Unit>
                </UnitSet>
                <IntegerDataEncoding sizeInBits="32"/>
            </IntegerParameterType>
            
            <IntegerParameterType name="degree_16">
                <UnitSet>
                    <Unit>°</Unit>
                </UnitSet>
                <IntegerDataEncoding sizeInBits="16"/>
            </IntegerParameterType>
        </ParameterTypeSet>
        


        <ParameterSet>
        <!-- THIS CONTAINS THE ACTUAL DATA-->
