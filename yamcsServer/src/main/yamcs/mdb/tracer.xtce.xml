        <MetaCommandSet>
            <MetaCommand name="header" abstract="true">
                <ArgumentList>
                    <Argument argumentTypeRef="uint_8" name="SYNC_1"></Argument>
                    <Argument argumentTypeRef="uint_8" name="SYNC_2"></Argument>
                    <Argument argumentTypeRef="uint_8" name="ID"></Argument>
                </ArgumentList>
                <CommandContainer name="header">
                    <EntryList>
                        <ArgumentRefEntry argumentRef="SYNC_1"></ArgumentRefEntry>
                        <ArgumentRefEntry argumentRef="SYNC_2"></ArgumentRefEntry>
                        <ArgumentRefEntry argumentRef="ID"></ArgumentRefEntry>
                    </EntryList>
                </CommandContainer>
            </MetaCommand>
            
            <MetaCommand name="set_state" shortDescription="Sets the state of the selected board">
                <BaseMetaCommand metaCommandRef="header">
                    <ArgumentAssignmentList>
                        <ArgumentAssignment argumentName="SYNC_1" argumentValue="0x42" />
                        <ArgumentAssignment argumentName="SYNC_2" argumentValue="0xac" />
                        <ArgumentAssignment argumentName="ID" argumentValue="0xa0" />
                    </ArgumentAssignmentList>
                </BaseMetaCommand>
                <ArgumentList>
                    <Argument argumentTypeRef="state_enum_type" name="state"></Argument>
                    <Argument argumentTypeRef="board_selector_type" name="board" shortDescription="Select board"></Argument>
                </ArgumentList>
