            <Parameter name="CCSDS_Packet_ID" parameterTypeRef="CCSDS_Packet_ID_Type" />
            <Parameter name="CCSDS_Packet_Sequence" parameterTypeRef="CCSDS_Packet_Sequence_Type" />
            <Parameter name="CCSDS_Packet_Length" parameterTypeRef="CCSDS_Packet_Length_Type" />
            <Parameter parameterTypeRef="uint_8" name="SYNC_1"/>
            <Parameter parameterTypeRef="uint_8" name="SYNC_2"/>
            <Parameter parameterTypeRef="uint_8" name="ID"/>
            <Parameter parameterTypeRef="switch_1_on" name="LO_state"/>
            <Parameter parameterTypeRef="switch_1_on" name="SODS_state"/>
            <Parameter parameterTypeRef="switch_1_on" name="SOE_state"/>
            <Parameter parameterTypeRef="fill" name="FILL"/>
            <Parameter parameterTypeRef="timer_type" name="EXP_timer"/>
            <Parameter parameterTypeRef="uint_16" name="CRC"/>

            <!-- STATUS DATASET-->
            <Parameter parameterTypeRef="switch_UPP" name="UPP_state"/>
