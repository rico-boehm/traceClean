            <Parameter parameterTypeRef="uint_8" name="DATA_error"/>
            <Parameter parameterTypeRef="uint_8" name="CRC_error"/>
        </ParameterSet>

        
        <ContainerSet>
            <SequenceContainer abstract="true" name="CCSDSPacket">
				<EntryList>
					<ParameterRefEntry parameterRef="CCSDS_Packet_ID" />
					<ParameterRefEntry parameterRef="CCSDS_Packet_Sequence" />
					<ParameterRefEntry parameterRef="CCSDS_Packet_Length" />
				</EntryList>
			</SequenceContainer>
            
            <SequenceContainer name="Packet_header" shortDescription="Packet Header of TRACE Telemetry" abstract="true">
                <EntryList>
                    <ParameterRefEntry parameterRef="SYNC_1"/>
                    <ParameterRefEntry parameterRef="SYNC_2"/>
                    <ParameterRefEntry parameterRef="ID"/>
                </EntryList>
                <BaseContainer containerRef="CCSDSPacket">
					<RestrictionCriteria>
						<ComparisonList>
                            <Comparison value="0" parameterRef="CCSDS_Packet_ID/Version" />
							<Comparison value="TM" parameterRef="CCSDS_Packet_ID/Type" />
                            <Comparison value="NotPresent" parameterRef="CCSDS_Packet_ID/SecHdrFlag" />
						</ComparisonList>
					</RestrictionCriteria>
				</BaseContainer>
            </SequenceContainer>

            <!-- STATUS PACKET-->
            <SequenceContainer name="Status_packet" shortDescription="Telemetry data for the status of the system">
                <EntryList>
                    <ParameterRefEntry parameterRef="LO_state"/>
                    <ParameterRefEntry parameterRef="SODS_state"/>
                    <ParameterRefEntry parameterRef="SOE_state"/>
                    <ParameterRefEntry parameterRef="UPP_state"/>
                    <ParameterRefEntry parameterRef="FILL"/>
                    <ParameterRefEntry parameterRef="EXP_timer"/>
                    <ParameterRefEntry parameterRef="CURR_3_3"/>
                    <ParameterRefEntry parameterRef="CURR_5"/>
                    <ParameterRefEntry parameterRef="CURR_12"/>
                    <ParameterRefEntry parameterRef="FLOW_gas"/>
                    <ParameterRefEntry parameterRef="FLOW_liq"/>
                    <ParameterRefEntry parameterRef="PRESS_tank"/>
                    <ParameterRefEntry parameterRef="PRESS_surf"/>
                    <ParameterRefEntry parameterRef="CRC"/>
                </EntryList>
                
                <BaseContainer containerRef="Packet_header">
					<RestrictionCriteria>
						<ComparisonList>
							<Comparison value="0x42" parameterRef="SYNC_1" />
                            <Comparison value="0xac" parameterRef="SYNC_2" />
                            <Comparison value="0xf0" parameterRef="ID"/>
						</ComparisonList>
					</RestrictionCriteria>
				</BaseContainer>
            </SequenceContainer>
            <!-- END OF STATUS PACKET-->

            <!-- HULL SENSOR PACKET 1-->
            <SequenceContainer name="Hull_sensor_packet_1" shortDescription="Telemetry data from the first set of hull sensors">
                <EntryList>
                    <ParameterRefEntry parameterRef="LO_state"/>
                    <ParameterRefEntry parameterRef="SODS_state"/>
                    <ParameterRefEntry parameterRef="SOE_state"/>
                    <ParameterRefEntry parameterRef="LOW_state"/>
                    <ParameterRefEntry parameterRef="FILL"/>
                    <ParameterRefEntry parameterRef="EXP_timer"/>
