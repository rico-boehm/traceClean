            <!-- END OF HULL SENSOR PACKET 2-->

            <!-- HULL SENSOR PACKET 3-->
            <SequenceContainer name="Hull_sensor_packet_3" shortDescription="Telemetry data from the third set of hull sensors">
                <EntryList>
                    <ParameterRefEntry parameterRef="LO_state"/>
                    <ParameterRefEntry parameterRef="SODS_state"/>
                    <ParameterRefEntry parameterRef="SOE_state"/>
                    <ParameterRefEntry parameterRef="LOW_state"/>
                    <ParameterRefEntry parameterRef="FILL"/>
                    <ParameterRefEntry parameterRef="EXP_timer"/>
                    <ParameterRefEntry parameterRef="TEMP_sensor_19"/>
                    <ParameterRefEntry parameterRef="TEMP_sensor_20"/>
                    <ParameterRefEntry parameterRef="TEMP_sensor_21"/>
                    <ParameterRefEntry parameterRef="TEMP_sensor_22"/>
                    <ParameterRefEntry parameterRef="TEMP_sensor_23"/>
                    <ParameterRefEntry parameterRef="TEMP_sensor_24"/>
                    <ParameterRefEntry parameterRef="TEMP_sensor_25"/>
                    <ParameterRefEntry parameterRef="TEMP_sensor_26"/>
                    <ParameterRefEntry parameterRef="TEMP_sensor_27"/>
                    <ParameterRefEntry parameterRef="CRC"/>
                </EntryList>
                
                <BaseContainer containerRef="Packet_header">
					<RestrictionCriteria>
						<ComparisonList>
							<Comparison value="0x42" parameterRef="SYNC_1" />
                            <Comparison value="0xac" parameterRef="SYNC_2" />
							<Comparison value="0xf4" parameterRef="ID"/>
						</ComparisonList>
					</RestrictionCriteria>
				</BaseContainer>
            </SequenceContainer>
            <!-- END OF HULL SENSOR PACKET 3-->

            <!-- ERROR CRC PACKET-->
            <SequenceContainer name="Error_crc_packet" shortDescription="Error faulty CRC code">
                <EntryList>
                    <ParameterRefEntry parameterRef="CRC_error"/>
                    <ParameterRefEntry parameterRef="CRC"/>
                </EntryList>
                
                <BaseContainer containerRef="Packet_header">
                    <RestrictionCriteria>
                        <ComparisonList>
                            <Comparison value="0x42" parameterRef="SYNC_1" />
                            <Comparison value="0xac" parameterRef="SYNC_2" />
                            <Comparison value="0xfa" parameterRef="ID" />
                        </ComparisonList>
                    </RestrictionCriteria>
                </BaseContainer>
            </SequenceContainer>
            <!-- END ERROR CRC PACKET-->

            <!-- INVALID DATA PACKET-->
            <SequenceContainer name="Error_data_packet" shortDescription="Error faulty payload">
                <EntryList>
                    <ParameterRefEntry parameterRef="DATA_error"/>
                    <ParameterRefEntry parameterRef="CRC"/>
                </EntryList>
                
                <BaseContainer containerRef="Packet_header">
                    <RestrictionCriteria>
                        <ComparisonList>
                            <Comparison value="0x42" parameterRef="SYNC_1" />
                            <Comparison value="0xac" parameterRef="SYNC_2" />
                            <Comparison value="0xff" parameterRef="ID" />
                        </ComparisonList>
                    </RestrictionCriteria>
                </BaseContainer>
            </SequenceContainer>
            <!-- END INVALID DATA PACKET-->
        </ContainerSet>
