                    <ParameterRefEntry parameterRef="TEMP_sensor_6"/>
                    <ParameterRefEntry parameterRef="TEMP_sensor_7"/>
                    <ParameterRefEntry parameterRef="TEMP_sensor_8"/>
                    <ParameterRefEntry parameterRef="TEMP_sensor_9"/>
